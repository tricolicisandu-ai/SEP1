<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="300.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CONTROLLER_NAME">
   <children>
      <VBox prefHeight="200.0" prefWidth="300.0" spacing="10.0">
         <children>
            <Label text="Current community Points">
               <VBox.margin>
                  <Insets left="20.0" top="20.0" />
               </VBox.margin>
            </Label>
            <TextField editable="false" prefHeight="25.0" prefWidth="144.0">
               <VBox.margin>
                  <Insets left="20.0" right="140.0" />
               </VBox.margin>
            </TextField>
         </children>
         <HBox.margin>
            <Insets top="70.0" />
         </HBox.margin>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="300.0">
         <children>
            <GridPane prefHeight="60.0" prefWidth="150.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="299.5" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.5" minWidth="10.0" prefWidth="300.5" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label prefHeight="26.0" prefWidth="300.0" text="Threshold Goal">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                  </Label>
                  <Label prefHeight="11.0" prefWidth="43.0" text="Points" GridPane.rowIndex="1" />
                  <TextField GridPane.columnIndex="1" />
                  <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
               <VBox.margin>
                  <Insets right="10.0" top="10.0" />
               </VBox.margin>
            </GridPane>
            <FlowPane alignment="CENTER" prefHeight="73.0" prefWidth="206.0">
               <children>
                  <Button mnemonicParsing="false" text="Add">
                     <FlowPane.margin>
                        <Insets left="40.0" />
                     </FlowPane.margin>
                  </Button>
               </children>
            </FlowPane>
         </children>
         <HBox.margin>
            <Insets top="70.0" />
         </HBox.margin>
      </VBox>
   </children>
</HBox>
